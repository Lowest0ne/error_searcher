{
  "manifest_version": 2,

  "name": "Error Search",
  "description": "Finds Ruby On Rails server errors on page and searches for answers",
  "version": "0.1",

  "permissions": [
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "results.html",
    "default_title": "Click Me"
  },

  "content_scripts": [ {
  "js": [ "jquery-1.10.2.min.js", "search.js"],
  "matches": [ "http://*/*", "https://*/*"]
  }]
}
