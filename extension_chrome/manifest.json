{
  "manifest_version": 2,

  "name": "Error Search",
  "description": "Finds Ruby On Rails server errors on page and searches for answers",
  "version": "0.1",

  "permissions": [
    "tabs",
    "activeTab"

  ],

  "background":{
    "page": "results.html"
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Search errors on Stack Overflow"
  }

}
